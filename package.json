    "node": "7.10.0"
  },
  "author": "nopeman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eepforwe/test-project/issues"
  },
  "homepage": "https://github.com/eepforwe/test-project#readme",
  "dependencies": {
    "debug": "*",
    "babel-cli": "*",
    "babel-core": "*",
    "babel-preset-env": "*",
    "babel-preset-stage-0": "*",
    "babel-polyfill": "*",
    "rollbar": "*",
    "nodemon": "*",
    "bootstrap": "^4.0.0-alpha.6",
    "koa": "*",
    "koa-router": "*",
    "koa-pug": "*",
    "koa-webpack": "*",
    "webpack": "*",
    "gulp": "*",
    "koa-static": "*",
    "koa-logger": "*",
    "koa-bodyparser": "*"
  },
  "devDependencies": {
